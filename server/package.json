{
  "name": "@inato/marketplace-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "index.js",
  "engines": {
    "node": "12.18.0"
  },
  "scripts": {
    "build": "yarn tsc -b && yarn copyfiles",
    "clean-build": "rimraf dist",
    "create-migration": "typeorm --config ../../ormconfig.ts migration:create",
    "copyfiles": "yarn copyfiles-scripts && yarn copyfiles-src",
    "copyfiles-scripts": "copyfiles -e 'scripts/**/*.ts' scripts/**/* dist",
    "copyfiles-src": "copyfiles -e 'src/**/*.ts' src/**/* dist",
    "db:clear": "yarn runScript --script=clearDatabase --skipMigrations",
    "db:demo": "yarn runScript --script=populateDatabase/insertDemoData",
    "db:populate": "yarn runScript --script=populateDatabase/insertLocalData",
    "db:staging": "yarn runScript --script=populateDatabase/insertStagingData",
    "demoSpreadsheetService": "yarn runScript --script=one-off/demoSpreadsheetService",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generate-domain-dependency-graph": "yarn depcruise --include-only '^src/domain' -x '(test.ts|Repository.ts|codec.ts|Factory.ts|Codec.ts|.codec.ts|.setupTest.ts)$' --output-type dot src | dot -T svg | yarn depcruise-wrap-stream-in-html > dependency-graph.html",
    "generate-migration": "yarn build && typeorm --config ../../ormconfig.ts migration:generate",
    "generate-service-token": "yarn runScript --script=generateServiceToken",
    "lint": "eslint --cache --ext .ts ./src ./scripts",
    "migrate": "yarn runScript --script=migrate",
    "notifyIncompleteApplications": "yarn runScript --script=notifyIncompleteApplications",
    "readThenWriteAllPairings": "yarn runScript --script=one-off/readThenWriteAllPairings",
    "readThenWriteAllSites": "yarn runScript --script=one-off/readThenWriteAllSites",
    "readThenWriteAllTrialPreferences": "yarn runScript --script=one-off/readThenWriteAllTrialPreferences",
    "readThenWriteAllTrials": "yarn runScript --script=one-off/readThenWriteAllTrials",
    "readThenWriteAllUsers": "yarn runScript --script=one-off/readThenWriteAllUsers",
    "refreshReportingData": "yarn runScript --script=refreshReportingData",
    "runScript": "node -r ./require-script.js ./run-script.js",
    "monitorApplicationKPIs": "yarn runScript --script=monitorApplicationKPIs",
    "schema": "yarn runScript --script=exportSchema",
    "start": "node --async-stack-traces -r ./require.js dist/src/index.js",
    "start-dev": "nodemon --config nodemon.json",
    "test": "jest",
    "test:coverage": "yarn test:unit --coverage && yarn test:spec --coverage && yarn test:forest --coverage",
    "test:forest": "jest --projects ./jest.forest.config.js",
    "test:spec": "jest --projects ./jest.spec.config.js",
    "test:unit": "jest --projects ./jest.unit.config.js",
    "updateSiteInvitations": "yarn runScript --script=one-off/updateSiteInvitations",
    "upsertAllSitesToCustomerIO": "yarn runScript --script=one-off/upsertAllSitesToCustomerIO",
    "validate": "yarn lint && yarn build && yarn test",
    "validate-dependencies": "yarn depcruise --validate .dependency-cruiser.js src scripts",
    "validate-schema": "./scripts/validateSchema.sh"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.1.1",
    "@inato/codec": "^0.0.1",
    "@inato/datasets": "1.0.0",
    "@inato/environment": "^1.0.1",
    "@inato/logging": "^1.0.3",
    "@inato/node-alerting": "^1.1.0",
    "@inato/node-feature-flags": "^1.0.1",
    "@nexus/schema": "^0.14.0",
    "@sentry/node": "^5.27.6",
    "apollo-server-express": "^2.16.1",
    "aws-sdk": "^2.730.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-parse": "^4.12.0",
    "customerio-node": "^0.7.0",
    "data-uri-to-buffer": "^3.0.1",
    "decod": "^6.1.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-bearer-token": "^2.4.0",
    "express-fileupload": "^1.2.0",
    "express-jwt": "^6.0.0",
    "forest-express-sequelize": "6.5.0",
    "fp-ts": "^2.9.1",
    "googleapis": "^65.0.0",
    "graphql": "15.3.0",
    "hellosign-sdk": "^1.6.0",
    "helmet": "^4.3.1",
    "io-ts": "^2.2.9",
    "is-ip": "^3.1.0",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "luxon": "^1.24.1",
    "monocle-ts": "^2.3.3",
    "multer": "^1.4.2",
    "nanoid": "^3.1.12",
    "newtype-ts": "^0.3.4",
    "pg": "^8.3.0",
    "rimraf": "^3.0.2",
    "sequelize": "6.3.0",
    "streamifier": "^0.1.1",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.25",
    "typeorm-naming-strategies": "^1.1.0",
    "uuid": "^7.0.3",
    "wait-port": "^0.2.9",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@inato/eslint-config-marketplace": "^1.0.0",
    "@relmify/jest-fp-ts": "^1.1.1",
    "@types/auth0": "^2.20.5",
    "@types/cookie-parser": "^1.4.2",
    "@types/copyfiles": "^2",
    "@types/express": "^4.17.7",
    "@types/express-fileupload": "^1.1.3",
    "@types/express-jwt": "^0.0.42",
    "@types/helmet": "^4",
    "@types/jest": "^26.0.15",
    "@types/jscodeshift": "^0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.150",
    "@types/luxon": "^1.24.1",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.14",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/yargs": "^15.0.5",
    "copyfiles": "^2.4.1",
    "csv-stringify": "^5.5.1",
    "datauri": "^3.0.0",
    "dependency-cruiser": "^9.8.0",
    "eslint": "^7.14.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-inato": "1.0.0",
    "jest": "^26.6.3",
    "jscodeshift": "^0.11.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "sqlite3": "^5.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.1",
    "ts-node": "^8.6.2",
    "typescript": "^4.1.2"
  }
}
