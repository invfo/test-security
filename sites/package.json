    "react-datepicker": "^3.1.3",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.7.0",
    "react-pdf": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-scroll": "^1.8.1",
