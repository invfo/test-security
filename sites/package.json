{
  "name": "@inato/sites-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:demo": "env-cmd -f .env.demo yarn build",
    "build:development": "env-cmd -f .env.development yarn build",
    "build:production": "env-cmd -f .env.production yarn build",
    "build:staging": "env-cmd -f .env.staging yarn build",
    "build": "node scripts/build.js",
    "compile-queries": "bash -c 'find . -name __generated__ | xargs rm -rf' && apollo client:codegen --target typescript --localSchemaFile ../server/schema.graphql --useReadOnlyTypes --globalTypesFile=src/__generated__/globalTypes.ts --passthroughCustomScalars --customScalarsPrefix Inato && graphql-codegen",
    "format": "yarn lint --fix",
    "build-firebase-json-file:demo": "env-cmd -f .env.demo yarn build-firebase-json-file",
    "build-firebase-json-file:production": "env-cmd -f .env.production yarn build-firebase-json-file",
    "build-firebase-json-file:staging": "env-cmd -f .env.staging yarn build-firebase-json-file",
    "build-firebase-json-file": "node scripts/build-firebase-json-file.js",
    "lint": "eslint --cache --ext .ts,.tsx ./src",
    "serve": "serve -s build -p 3000",
    "start-dev": "yarn start",
    "start": "node scripts/start.js",
    "test": "jest",
    "typecheck": "yarn tsc",
    "validate-compiled-queries": "./scripts/validateCompiledQueries.sh",
    "validate-dependencies": "yarn depcruise --validate .dependency-cruiser.js src",
    "validate": "yarn lint && yarn typecheck && yarn test --watchAll=false"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "nohoist": [
    "@types/*",
    "jest"
  ],
  "dependencies": {
    "@apollo/client": "^3.1.4",
    "@babel/core": "7.9.0",
    "@hookform/resolvers": "^1.0.0",
    "@inato/browser-feature-flags": "^1.1.2",
    "@inato/browser-tracking": "^1.4.6",
    "@inato/datasets": "^1.0.0",
    "@inato/environment": "^1.0.1",
    "@inato/feasibility-ui-notifications": "2.0.1",
    "@inato/logging": "^1.0.3",
    "@inato/ui": "5.48.0",
    "@slack/web-api": "^6.0.0",
